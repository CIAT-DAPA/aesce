<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	android:scrollbarAlwaysDrawHorizontalTrack="true" android:background="#FFFFFF"
	android:orientation="vertical">
	<ScrollView android:layout_height="wrap_content" android:id="@+id/scrollView1" android:layout_width="match_parent" android:layout_marginTop="10sp">
		<TableLayout android:layout_width="match_parent" android:id="@+id/tableLayout1" android:layout_height="match_parent" android:layout_marginTop="20sp" android:layout_marginRight="10sp">
			<TableRow android:id="@+id/tableRow4" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="5sp" android:layout_marginRight="10sp">
				<LinearLayout android:layout_marginTop="20sp" android:id="@+id/linearLayout5" android:layout_height="wrap_content" android:layout_width="match_parent">
					<Button android:id="@+id/IdFincaBtnRegresar" android:layout_height="wrap_content" android:text="@string/TextProductorBtnRegresar" android:textSize="20sp" android:layout_width="wrap_content" android:onClick="OnFincarBtnRegresar_Click"></Button>
					<Button android:id="@+id/button2" android:layout_height="wrap_content" android:text="@string/TEXTproductorBtnSalir" android:textSize="20sp" android:layout_width="wrap_content"></Button>
				</LinearLayout>
			</TableRow>
			<TableRow android:id="@+id/tableRow1" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="5sp" android:layout_marginRight="10sp">
				<LinearLayout android:layout_marginTop="10sp" android:id="@+id/linearLayout4" android:layout_height="wrap_content" android:layout_width="wrap_content">
					<GridView android:transcriptMode="alwaysScroll" android:isScrollContainer="true" android:horizontalSpacing="1sp" android:scrollbarAlwaysDrawVerticalTrack="true" android:id="@+id/IdFincaGdvFinca" android:textFilterEnabled="true" android:scrollbarSize="10sp" android:stretchMode="columnWidth" android:numColumns="2" android:fadeScrollbars="true" android:scrollbars="vertical" android:verticalSpacing="1sp" android:focusableInTouchMode="true" android:layout_height="200sp" android:background="#000000" android:smoothScrollbar="true" android:layout_width="wrap_content" android:focusable="true" android:columnWidth="80sp" android:layout_marginLeft="2sp" android:layout_marginRight="10sp"></GridView>
				</LinearLayout>
			</TableRow>
			<TableRow android:id="@+id/tableRow2" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="5sp" android:layout_marginRight="10sp">
				<LinearLayout android:id="@+id/linearLayout6" android:layout_height="wrap_content" android:layout_width="match_parent" android:layout_marginTop="10sp">
					<Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/IdFincaBtnNuevaFinca" android:text="@string/TEXTfincaBtnNuevaFinca" android:onClick="OnFincaInsertar_Click"></Button>
				</LinearLayout>
			</TableRow>
			<TableRow android:id="@+id/tableRow3" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="5sp" android:layout_marginRight="10sp">
				<LinearLayout android:id="@+id/linearLayout3" android:layout_height="wrap_content" android:layout_width="match_parent" android:layout_marginTop="30sp">
					<ImageView android:src="@drawable/asohofrucol" android:layout_marginLeft="10sp" android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/imageView2"></ImageView>
					<ImageView android:src="@drawable/um" android:layout_marginLeft="10sp" android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/imageView3"></ImageView>
					<ImageView android:src="@drawable/fondo" android:layout_marginLeft="10sp" android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/imageView4"></ImageView>
				</LinearLayout>
			</TableRow>
		</TableLayout>
	</ScrollView>
</LinearLayout>
