<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	android:orientation="vertical" android:background="#FFFFFF">
	<LinearLayout android:id="@+id/linearLayout1"
		android:layout_width="match_parent" android:layout_height="wrap_content">
		<TextView android:id="@+id/textView1" android:layout_height="wrap_content"
			android:background="#7D4312" android:textColor="#FFFFFF" android:layout_width="match_parent"
			android:gravity="center" android:height="40sp" android:textSize="15sp" android:text="Hoja de Procedimientos (Materia Orgánica)"></TextView>
	</LinearLayout>
	<ScrollView android:id="@+id/scrollView1"
		android:layout_marginTop="10sp" android:layout_width="match_parent"
		android:layout_height="match_parent">
		<LinearLayout android:id="@+id/linearLayout4"
			android:layout_width="match_parent" android:layout_height="match_parent">
			<TableLayout android:layout_width="match_parent"
				android:id="@+id/tableLayout1" android:layout_height="match_parent"
				android:layout_marginLeft="5sp" android:layout_marginRight="10sp">
				<TableRow android:layout_width="wrap_content" android:id="@+id/tableRow3"
					android:layout_height="wrap_content">
					<LinearLayout android:layout_height="wrap_content"
						android:id="@+id/linearLayout5" android:layout_width="match_parent">
						<Button android:textSize="20sp" android:layout_width="wrap_content"
							android:text="@string/TextProductorBtnRegresar"
							android:layout_height="wrap_content" android:id="@+id/IdModRastaHrProcedimientosMatOrganicaBtnRegresar" android:onClick="OnModRastaHrProcedimientosMatOrganicaBtnRegresar_Click"></Button>
						<Button android:id="@+id/button2" android:textSize="20sp"
							android:layout_width="wrap_content" android:text="@string/TEXTproductorBtnSalir"
							android:layout_height="wrap_content"></Button>
					</LinearLayout>
				</TableRow>
				<TableRow android:layout_width="wrap_content" android:id="@+id/tableRow1"
					android:layout_height="wrap_content">
					<LinearLayout android:layout_height="wrap_content"
						android:id="@+id/linearLayout3" android:layout_marginTop="20sp"
						android:orientation="vertical" android:layout_width="fill_parent">
						<TextView android:width="300sp" android:gravity="center" android:background="#dca76d" android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/textView2" android:textColor="#704312" android:height="30sp" android:text="Materia Orgánica"></TextView>
					</LinearLayout>
				</TableRow>
				<TableRow android:layout_width="wrap_content" android:id="@+id/TableRow01" android:layout_height="wrap_content" android:layout_marginTop="10sp">
					<LinearLayout android:layout_width="wrap_content" android:orientation="vertical" android:id="@+id/LinearLayout01" android:layout_height="wrap_content">
						<TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/TextView01" android:layout_marginRight="5sp" android:textColor="#000000" android:textSize="12sp" android:text="Observa mucha hojarasta o materia orgánica en descomposición? (ver numeral 19)"></TextView>
						<RadioGroup android:orientation="horizontal" android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/IdModRastaHrProcedimientosMatOrganicaRdgPreg1">
							<RadioButton android:layout_width="wrap_content" android:layout_height="wrap_content" android:checked="true" android:textColor="#000000" android:text="Si" android:layout_marginRight="5sp" android:id="@+id/IdModRastaHrProcedimientosMatOrganicaRdoPreg1SI"></RadioButton>
							<RadioButton android:layout_width="wrap_content" android:layout_height="wrap_content" android:textColor="#000000" android:text="No" android:id="@+id/IdModRastaHrProcedimientosMatOrganicaRdoPreg1NO"></RadioButton>
						</RadioGroup>
					</LinearLayout>
				</TableRow>
				<TableRow android:layout_marginTop="10sp" android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/TableRow02">
					<LinearLayout android:layout_height="wrap_content" android:id="@+id/LinearLayout02" android:orientation="vertical" android:layout_width="wrap_content">
						<TextView android:layout_marginRight="5sp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:textColor="#000000" android:textSize="12sp" android:id="@+id/TextView02" android:text="Cuando introduce el cuchillo en el primer horizonte, éste entra sin ningún esfuerzo? (ver numeral 21)"></TextView>
						<RadioGroup android:layout_width="wrap_content" android:layout_height="wrap_content" android:orientation="horizontal" android:id="@+id/IdModRastaHrProcedimientosMatOrganicaRdgPreg2">
							<RadioButton android:layout_marginRight="5sp" android:checked="true" android:layout_width="wrap_content" android:text="Si" android:layout_height="wrap_content" android:textColor="#000000" android:id="@+id/IdModRastaHrProcedimientosMatOrganicaRdoPreg2SI"></RadioButton>
							<RadioButton android:layout_width="wrap_content" android:text="No" android:layout_height="wrap_content" android:textColor="#000000" android:id="@+id/IdModRastaHrProcedimientosMatOrganicaRdoPreg2NO"></RadioButton>
						</RadioGroup>
					</LinearLayout>
				</TableRow>
				<TableRow android:layout_marginTop="10sp" android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/TableRow03">
					<LinearLayout android:layout_height="wrap_content" android:id="@+id/LinearLayout05" android:orientation="vertical" android:layout_width="wrap_content">
						<TextView android:layout_marginRight="5sp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:textColor="#000000" android:textSize="12sp" android:id="@+id/TextView03" android:text="Encontró un PH muy ácido (Ver numeral 4)"></TextView>
						<RadioGroup android:layout_width="wrap_content" android:layout_height="wrap_content" android:orientation="horizontal" android:id="@+id/IdModRastaHrProcedimientosMatOrganicaRdgPreg3">
							<RadioButton android:layout_marginRight="5sp" android:checked="true" android:layout_width="wrap_content" android:text="Si" android:layout_height="wrap_content" android:textColor="#000000" android:id="@+id/IdModRastaHrProcedimientosMatOrganicaRdoPreg3SI"></RadioButton>
							<RadioButton android:layout_width="wrap_content" android:text="No" android:layout_height="wrap_content" android:textColor="#000000" android:id="@+id/IdModRastaHrProcedimientosMatOrganicaRdoPreg3NO"></RadioButton>
						</RadioGroup>
					</LinearLayout>
				</TableRow>
				<TableRow android:layout_marginTop="10sp" android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/TableRow04">
					<LinearLayout android:layout_height="wrap_content" android:id="@+id/LinearLayout06" android:orientation="vertical" android:layout_width="wrap_content">
						<TextView android:layout_marginRight="5sp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:textColor="#000000" android:textSize="12sp" android:id="@+id/TextView04" android:text="Su drenaje es pobre? (Hoja de Respuestas 2)"></TextView>
						<RadioGroup android:layout_width="wrap_content" android:layout_height="wrap_content" android:orientation="horizontal" android:id="@+id/IdModRastaHrProcedimientosMatOrganicaRdgPreg4">
							<RadioButton android:layout_marginRight="5sp" android:checked="true" android:layout_width="wrap_content" android:text="Si" android:layout_height="wrap_content" android:textColor="#000000" android:id="@+id/IdModRastaHrProcedimientosMatOrganicaRdoPreg4SI"></RadioButton>
							<RadioButton android:layout_width="wrap_content" android:text="No" android:layout_height="wrap_content" android:textColor="#000000" android:id="@+id/IdModRastaHrProcedimientosMatOrganicaRdoPreg4NO"></RadioButton>
						</RadioGroup>
					</LinearLayout>
				</TableRow>
				<TableRow android:layout_width="wrap_content" android:id="@+id/TableRow08" android:layout_marginTop="10sp" android:layout_height="wrap_content">
					<LinearLayout android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/LinearLayout03" android:orientation="vertical">
						<Button android:layout_width="wrap_content" android:textSize="20sp" android:layout_height="wrap_content" android:text="Registrar" android:id="@+id/IdModRastaHrProcedimientosMatOrganicaBtnRegistrar" android:onClick="OnModRastaHrProcedimientosMatOrganicaBtnRegistrar_Click"></Button>
					</LinearLayout>
				</TableRow>
				<TableRow android:layout_width="wrap_content" android:id="@+id/tableRow2" android:layout_marginRight="10sp" android:layout_height="wrap_content">
					<LinearLayout android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/LinearLayout04" android:layout_marginTop="30sp" android:layout_marginLeft="5sp" android:layout_marginBottom="10sp" android:layout_marginRight="10sp">
						<ImageView android:layout_width="wrap_content" android:layout_marginLeft="10sp" android:id="@+id/imageView2" android:src="@drawable/asohofrucol" android:layout_height="wrap_content"></ImageView>
						<ImageView android:layout_width="wrap_content" android:layout_marginLeft="10sp" android:id="@+id/imageView3" android:src="@drawable/um" android:layout_height="wrap_content"></ImageView>
						<ImageView android:layout_width="wrap_content" android:layout_marginLeft="10sp" android:id="@+id/imageView4" android:src="@drawable/fondo" android:layout_height="wrap_content"></ImageView>
					</LinearLayout>
				</TableRow>
			</TableLayout>
		</LinearLayout>
	</ScrollView>
</LinearLayout>
