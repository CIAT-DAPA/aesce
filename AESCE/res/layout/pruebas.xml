<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	android:orientation="vertical" android:background="#FFFFFF">
	<ScrollView android:id="@+id/scrollView1"
		android:layout_marginTop="10sp" android:layout_width="match_parent"
		android:layout_height="match_parent">
		<LinearLayout android:id="@+id/linearLayout4"
			android:layout_width="match_parent" android:layout_height="match_parent">
			<TableLayout android:layout_width="match_parent"
				android:id="@+id/tableLayout1" android:layout_height="match_parent"
				android:layout_marginLeft="5sp" android:layout_marginRight="10sp">
				<TableRow android:layout_height="wrap_content" android:id="@+id/TableRow06" android:layout_width="wrap_content">
					<LinearLayout android:layout_height="wrap_content" android:layout_width="fill_parent" android:id="@+id/LinearLayout06" android:layout_marginTop="20sp" android:orientation="vertical">
						<TextView android:textColor="#704312" android:layout_width="wrap_content" android:gravity="center" android:width="300sp" android:layout_height="wrap_content" android:id="@+id/TextView05" android:background="#dca76d" android:height="30sp" android:text="Moteados"></TextView>
					</LinearLayout>
				</TableRow>
				<TableRow android:layout_width="wrap_content" android:id="@+id/tableRow4"
					android:layout_height="wrap_content" android:layout_marginTop="10sp">
					<LinearLayout android:layout_width="wrap_content" android:id="@+id/LinearLayout07" android:layout_height="wrap_content" android:orientation="horizontal">
						<RadioGroup android:id="@+id/Idmodrastahr1caryobserv456RdgHorPedRoc" android:layout_width="wrap_content" android:orientation="horizontal" android:layout_height="wrap_content">
							<RadioButton android:checked="true" android:textColor="#000000" android:layout_height="wrap_content" android:layout_width="wrap_content" android:text="Si" android:id="@+id/Idmodrastahr1caryobserv456RdoHorPedRocSI"></RadioButton>
							<RadioButton android:layout_marginLeft="10sp" android:textColor="#000000" android:layout_height="wrap_content" android:layout_width="wrap_content" android:text="No" android:id="@+id/Idmodrastahr1caryobserv456RdoHorPedRocNO"></RadioButton>
						</RadioGroup>
					</LinearLayout>
				</TableRow>
				<TableRow android:layout_width="wrap_content" android:id="@+id/TableRow01"
					android:layout_height="wrap_content" android:layout_marginTop="10sp">
					<LinearLayout android:layout_height="wrap_content"
						android:id="@+id/LinearLayout01" android:layout_width="wrap_content"
						android:orientation="vertical">
						<TextView android:textColor="#000000" android:layout_width="wrap_content" android:textSize="20sp" android:layout_height="wrap_content" android:id="@+id/TextView01" android:layout_marginRight="5sp" android:text="Profundidad (cm)"></TextView>
						<EditText android:layout_width="match_parent" android:id="@+id/EditText01" android:inputType="numberDecimal" android:layout_height="wrap_content">
							<requestFocus></requestFocus>
						</EditText>
					</LinearLayout>
				</TableRow>
				<TableRow android:layout_width="wrap_content" android:id="@+id/TableRow02"
					android:layout_height="wrap_content" android:layout_marginTop="10sp">
					<LinearLayout android:layout_height="wrap_content"
						android:id="@+id/LinearLayout02" android:layout_width="wrap_content"
						android:orientation="vertical">
						<TextView android:textColor="#000000" android:layout_width="wrap_content" android:textSize="20sp" android:layout_height="wrap_content" android:id="@+id/TextView02" android:layout_marginRight="5sp" android:text="Moteados a mas de 70 cm"></TextView>
						<RadioGroup android:id="@+id/RadioGroup01" android:layout_width="wrap_content" android:orientation="horizontal" android:layout_height="wrap_content">
							<RadioButton android:checked="true" android:textColor="#000000" android:layout_height="wrap_content" android:layout_width="wrap_content" android:text="Si" android:id="@+id/RadioButton02"></RadioButton>
							<RadioButton android:layout_marginLeft="10sp" android:textColor="#000000" android:layout_height="wrap_content" android:layout_width="wrap_content" android:text="No" android:id="@+id/RadioButton01"></RadioButton>
						</RadioGroup>
					</LinearLayout>
				</TableRow>
				<TableRow android:layout_marginTop="10sp" android:layout_height="wrap_content" android:id="@+id/TableRow04" android:layout_width="wrap_content">
					<LinearLayout android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/LinearLayout04" android:orientation="vertical">
						<TextView android:textColor="#000000" android:layout_width="wrap_content" android:textSize="20sp" android:layout_height="wrap_content" android:id="@+id/TextView03" android:layout_marginRight="5sp" android:text="Estructura"></TextView>
						<Spinner android:layout_width="match_parent" android:id="@+id/Idmodrastahr1caryobserv456SpnPedSup" android:layout_height="wrap_content"></Spinner>
					</LinearLayout>
				</TableRow>
				<TableRow android:layout_marginTop="10sp" android:layout_height="wrap_content" android:id="@+id/TableRow11" android:layout_width="wrap_content">
					<LinearLayout android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/LinearLayout11" android:orientation="vertical">
						<Button android:layout_width="wrap_content" android:onClick="OnModRastaHr1CarYObserv456BtnRegistrar_Click" android:textSize="20sp" android:layout_height="wrap_content" android:text="Adicionar" android:id="@+id/Button01"></Button>
					</LinearLayout>
				</TableRow>
				<TableRow android:layout_width="wrap_content" android:id="@+id/tableRow2"
					android:layout_height="wrap_content" android:layout_marginRight="10sp">
					<LinearLayout android:layout_height="wrap_content"
						android:id="@+id/linearLayout3" android:layout_marginTop="30sp"
						android:layout_width="wrap_content" android:layout_marginRight="10sp"
						android:layout_marginLeft="5sp" android:layout_marginBottom="10sp">
						<ImageView android:layout_marginLeft="10sp" android:id="@+id/imageView2"
							android:src="@drawable/asohofrucol" android:layout_width="wrap_content"
							android:layout_height="wrap_content"></ImageView>
						<ImageView android:layout_marginLeft="10sp" android:id="@+id/imageView3"
							android:src="@drawable/um" android:layout_width="wrap_content"
							android:layout_height="wrap_content"></ImageView>
						<ImageView android:layout_marginLeft="10sp" android:id="@+id/imageView4"
							android:src="@drawable/fondo" android:layout_width="wrap_content"
							android:layout_height="wrap_content"></ImageView>
					</LinearLayout>
				</TableRow>
			</TableLayout>
		</LinearLayout>
	</ScrollView>
</LinearLayout>
