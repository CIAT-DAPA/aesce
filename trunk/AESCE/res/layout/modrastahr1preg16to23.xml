<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	android:orientation="vertical" android:background="#FFFFFF">
	<LinearLayout android:id="@+id/linearLayout1"
		android:layout_width="match_parent" android:layout_height="wrap_content">
		<TextView android:id="@+id/textView1" android:layout_height="wrap_content"
			android:background="#7D4312" android:text="Hoja de Respuestas No. 1"
			android:textColor="#FFFFFF" android:layout_width="match_parent"
			android:gravity="center" android:textSize="20sp" android:height="40sp"></TextView>
	</LinearLayout>
	<ScrollView android:id="@+id/scrollView1"
		android:layout_marginTop="10sp" android:layout_width="match_parent"
		android:layout_height="match_parent">
		<LinearLayout android:id="@+id/linearLayout4"
			android:layout_width="match_parent" android:layout_height="match_parent">
			<TableLayout android:layout_width="match_parent"
				android:id="@+id/tableLayout1" android:layout_height="match_parent"
				android:layout_marginLeft="5sp" android:layout_marginRight="10sp">
				<TableRow android:layout_width="wrap_content" android:id="@+id/tableRow3"
					android:layout_height="wrap_content">
					<LinearLayout android:layout_height="wrap_content"
						android:id="@+id/linearLayout5" android:layout_width="match_parent">
						<Button android:textSize="20sp" android:layout_width="wrap_content"
							android:text="@string/TextProductorBtnRegresar"
							android:layout_height="wrap_content" android:id="@+id/Idmodrastahr1preg16to23BtnRegresar" android:onClick="Onmodrastahr1preg16to23BtnRegresar_Cllick"></Button>
						<Button android:id="@+id/button2" android:textSize="20sp"
							android:layout_width="wrap_content" android:text="@string/TEXTproductorBtnSalir"
							android:layout_height="wrap_content"></Button>
					</LinearLayout>
				</TableRow>
				<TableRow android:layout_width="wrap_content" android:id="@+id/tableRow1"
					android:layout_height="wrap_content">
					<LinearLayout android:layout_height="wrap_content"
						android:id="@+id/linearLayout3" android:layout_marginTop="20sp"
						android:orientation="vertical" android:layout_width="fill_parent">
						<TextView android:id="@+id/textView2"
							android:layout_height="wrap_content" android:textColor="#704312"
							android:background="#dca76d" android:width="300sp"
							android:gravity="center" android:height="30sp"
							android:layout_width="wrap_content" android:text="Preguntas (16- 23)"></TextView>
					</LinearLayout>
				</TableRow>
				<TableRow android:layout_width="wrap_content" android:id="@+id/tableRow4"
					android:layout_height="wrap_content" android:layout_marginTop="10sp">
					<LinearLayout android:layout_width="wrap_content"
						android:id="@+id/LinearLayout07" android:layout_height="wrap_content"
						android:orientation="vertical">
						<TextView android:id="@+id/TextView05" android:textSize="15sp" android:text="16. ¿Se encuentra en una región seca (Árida) donde casi no llueve o una región húmeda cercana al mar o lagos salados?" android:textColor="#000000" android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_marginRight="5sp"></TextView>
						<RadioGroup android:layout_width="wrap_content" android:orientation="horizontal" android:layout_height="wrap_content" android:id="@+id/Idmodrastahr1preg16to23Rdg16">
							<RadioButton android:textColor="#000000" android:layout_height="wrap_content" android:layout_width="wrap_content" android:textSize="15sp" android:text="Si" android:checked="true" android:id="@+id/Idmodrastahr1preg16to23Rdo16SI"></RadioButton>
							<RadioButton android:textColor="#000000" android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_marginLeft="10sp" android:textSize="15sp" android:text="No" android:id="@+id/Idmodrastahr1preg16to23Rdo16NO"></RadioButton>
						</RadioGroup>
					</LinearLayout>
				</TableRow>
				<TableRow android:layout_width="wrap_content" android:id="@+id/TableRow01"
					android:layout_height="wrap_content" android:layout_marginTop="10sp">
					<LinearLayout android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/linearLayout2" android:orientation="vertical">
						<LinearLayout android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/LinearLayout06" android:orientation="vertical">
							<TextView android:id="@+id/TextView07" android:textSize="15sp" android:text="17. ¿Observa raíces vivas en el perfil?" android:layout_marginRight="5sp" android:layout_width="wrap_content" android:textColor="#000000" android:layout_height="wrap_content"></TextView>
							<RadioGroup android:layout_width="wrap_content" android:orientation="horizontal" android:layout_height="wrap_content" android:id="@+id/Idmodrastahr1preg16to23Rdg17">
								<RadioButton android:textColor="#000000" android:layout_height="wrap_content" android:layout_width="wrap_content" android:textSize="15sp" android:text="Si" android:checked="true" android:id="@+id/Idmodrastahr1preg16to23Rdo17SI"></RadioButton>
								<RadioButton android:textColor="#000000" android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_marginLeft="10sp" android:textSize="15sp" android:text="No" android:id="@+id/Idmodrastahr1preg16to23Rdo17NO"></RadioButton>
							</RadioGroup>
						</LinearLayout>
						<LinearLayout android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/LinearLayout01" android:orientation="horizontal">
							<TextView android:id="@+id/TextView01" android:layout_marginRight="5sp" android:layout_width="wrap_content" android:textColor="#000000" android:layout_height="wrap_content" android:text="Profundidad de las raíces (cm):" android:textSize="12sp"></TextView>
							<EditText android:layout_weight="1" android:layout_height="wrap_content" android:inputType="numberDecimal" android:layout_width="match_parent" android:width="200sp" android:id="@+id/Idmodrastahr1preg16to23EdtProf17">
								<requestFocus></requestFocus>
							</EditText>
						</LinearLayout>
					</LinearLayout>
				</TableRow>
				<TableRow android:layout_width="wrap_content" android:id="@+id/TableRow02"
					android:layout_height="wrap_content" android:layout_marginTop="10sp">
					<LinearLayout android:layout_height="wrap_content"
						android:id="@+id/LinearLayout02" android:layout_width="wrap_content"
						android:orientation="vertical">
						<TextView android:layout_width="wrap_content" android:id="@+id/TextView02"
							android:textColor="#000000" android:textSize="15sp"
							android:layout_marginRight="5sp" android:layout_height="wrap_content" android:text="18. ¿Observa en el cultivo plantas pequeñas, secas o de baja produció?"></TextView>
						<Spinner android:layout_width="match_parent"
							android:layout_height="wrap_content" android:id="@+id/Idmodrastahr1preg16to23spn18"></Spinner>
					</LinearLayout>
				</TableRow>
				<TableRow android:layout_width="wrap_content" android:id="@+id/TableRow09"
					android:layout_marginTop="10sp" android:layout_height="wrap_content">
					<LinearLayout android:layout_height="wrap_content"
						android:layout_width="wrap_content" android:id="@+id/LinearLayout09"
						android:orientation="vertical">
						<TextView android:layout_height="wrap_content" android:id="@+id/TextView03" android:layout_width="wrap_content" android:textSize="15sp" android:text="19. ¿Observa mucha hojarasca o materia orgánica en descomposición en la superficie del suelo?" android:textColor="#000000" android:layout_marginRight="5sp"></TextView>
						<RadioGroup android:layout_width="wrap_content" android:orientation="horizontal" android:layout_height="wrap_content" android:id="@+id/Idmodrastahr1preg16to23Rdg19">
							<RadioButton android:textColor="#000000" android:layout_height="wrap_content" android:layout_width="wrap_content" android:textSize="15sp" android:text="Si" android:checked="true" android:id="@+id/Idmodrastahr1preg16to23Rdo19SI"></RadioButton>
							<RadioButton android:textColor="#000000" android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_marginLeft="10sp" android:textSize="15sp" android:text="No" android:id="@+id/Idmodrastahr1preg16to23Rdo19NO"></RadioButton>
						</RadioGroup>
					</LinearLayout>
				</TableRow>
				<TableRow android:layout_width="wrap_content" android:id="@+id/TableRow04"
					android:layout_marginTop="10sp" android:layout_height="wrap_content">
					<LinearLayout android:layout_height="wrap_content"
						android:layout_width="wrap_content" android:id="@+id/LinearLayout08"
						android:orientation="vertical">
						<TextView android:id="@+id/TextView06" android:textSize="15sp" android:text="20. ¿El suelo es muy negro, muy blando, esponjoso y al caminar sobre él se hunde?" android:layout_marginRight="5sp" android:layout_width="wrap_content" android:textColor="#000000" android:layout_height="wrap_content"></TextView>
						<RadioGroup android:layout_width="wrap_content" android:orientation="horizontal" android:layout_height="wrap_content" android:id="@+id/Idmodrastahr1preg16to23Rdg20">
							<RadioButton android:textColor="#000000" android:layout_height="wrap_content" android:layout_width="wrap_content" android:textSize="15sp" android:text="Si" android:checked="true" android:id="@+id/Idmodrastahr1preg16to23Rdo20SI"></RadioButton>
							<RadioButton android:textColor="#000000" android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_marginLeft="10sp" android:textSize="15sp" android:text="No" android:id="@+id/Idmodrastahr1preg16to23Rdo20NO"></RadioButton>
						</RadioGroup>
					</LinearLayout>
				</TableRow>
				<TableRow android:layout_width="wrap_content" android:id="@+id/TableRow03"
					android:layout_marginTop="10sp" android:layout_height="wrap_content">
					<LinearLayout android:layout_height="wrap_content"
						android:layout_width="wrap_content" android:id="@+id/LinearLayout05"
						android:orientation="vertical">
						<TextView android:id="@+id/TextView04" android:textSize="15sp" android:text="21. Cuando introduce el cuchillo en el primer horizonte, ¿éste entra sin ningún esfuerzo?" android:layout_marginRight="5sp" android:layout_width="wrap_content" android:textColor="#000000" android:layout_height="wrap_content"></TextView>
						<RadioGroup android:layout_width="wrap_content" android:orientation="horizontal" android:layout_height="wrap_content" android:id="@+id/Idmodrastahr1preg16to23Rdg21">
							<RadioButton android:textColor="#000000" android:layout_height="wrap_content" android:layout_width="wrap_content" android:textSize="15sp" android:text="Si" android:checked="true" android:id="@+id/Idmodrastahr1preg16to23Rdo21SI"></RadioButton>
							<RadioButton android:textColor="#000000" android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_marginLeft="10sp" android:textSize="15sp" android:text="No" android:id="@+id/Idmodrastahr1preg16to23Rdo21NO"></RadioButton>
						</RadioGroup>
					</LinearLayout>
				</TableRow>
				<TableRow android:id="@+id/TableRow05" android:layout_width="wrap_content" android:layout_marginTop="10sp" android:layout_height="wrap_content">
					<LinearLayout android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/LinearLayout03" android:orientation="vertical">
						<TextView android:id="@+id/TextView08" android:textSize="15sp" android:layout_marginRight="5sp" android:layout_width="wrap_content" android:textColor="#000000" android:layout_height="wrap_content" android:text="22. ¿Se encuentra cerca de ríos, quebradas, mares, lagos o pozos qe mantienen el nivel del agua subterránea muy superficial?"></TextView>
						<RadioGroup android:layout_width="wrap_content" android:orientation="horizontal" android:layout_height="wrap_content" android:id="@+id/Idmodrastahr1preg16to23Rdg22">
							<RadioButton android:textColor="#000000" android:layout_height="wrap_content" android:layout_width="wrap_content" android:textSize="15sp" android:text="Si" android:checked="true" android:id="@+id/Idmodrastahr1preg16to23Rdo22SI"></RadioButton>
							<RadioButton android:textColor="#000000" android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_marginLeft="10sp" android:textSize="15sp" android:text="No" android:id="@+id/Idmodrastahr1preg16to23Rdo22NO"></RadioButton>
						</RadioGroup>
					</LinearLayout>
				</TableRow>
				<TableRow android:layout_height="wrap_content" android:id="@+id/TableRow06" android:layout_width="wrap_content" android:layout_marginTop="10sp">
					<LinearLayout android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/LinearLayout10" android:orientation="vertical">
						<TextView android:id="@+id/TextView09" android:textSize="15sp" android:layout_marginRight="5sp" android:layout_width="wrap_content" android:textColor="#000000" android:layout_height="wrap_content" android:text="23. El recubrimiento vegetal del suelo, como pastos, malezas, césped o musgo, es:"></TextView>
						<Spinner android:layout_width="match_parent" android:layout_height="wrap_content" android:id="@+id/Idmodrastahr1preg16to23spn23"></Spinner>
					</LinearLayout>
				</TableRow>
				<TableRow android:id="@+id/TableRow08" android:layout_marginTop="10sp" android:layout_width="wrap_content" android:layout_height="wrap_content">
					<LinearLayout android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/LinearLayout11" android:orientation="vertical">
						<Button android:layout_width="wrap_content" android:text="Registrar" android:textSize="20sp" android:layout_height="wrap_content" android:id="@+id/Idmodrastahr1preg16to23BtnRegistrar" android:onClick="Onmodrastahr1preg16to23BtnRegistrar_Click"></Button>
					</LinearLayout>
				</TableRow>
				<TableRow android:layout_width="wrap_content" android:id="@+id/tableRow2"
					android:layout_marginRight="10sp" android:layout_height="wrap_content">
					<LinearLayout android:layout_height="wrap_content"
						android:layout_width="wrap_content" android:id="@+id/LinearLayout04"
						android:layout_marginTop="30sp" android:layout_marginLeft="5sp"
						android:layout_marginBottom="10sp" android:layout_marginRight="10sp">
						<ImageView android:layout_width="wrap_content"
							android:layout_marginLeft="10sp" android:id="@+id/imageView2"
							android:src="@drawable/asohofrucol" android:layout_height="wrap_content"></ImageView>
						<ImageView android:layout_width="wrap_content"
							android:layout_marginLeft="10sp" android:id="@+id/imageView3"
							android:src="@drawable/um" android:layout_height="wrap_content"></ImageView>
						<ImageView android:layout_width="wrap_content"
							android:layout_marginLeft="10sp" android:id="@+id/imageView4"
							android:src="@drawable/fondo" android:layout_height="wrap_content"></ImageView>
					</LinearLayout>
				</TableRow>
			</TableLayout>
		</LinearLayout>
	</ScrollView>
</LinearLayout>
