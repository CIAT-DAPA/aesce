<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	android:scrollbarAlwaysDrawHorizontalTrack="true" android:background="#FFFFFF"
	android:orientation="vertical">
	<ScrollView android:layout_height="wrap_content" android:id="@+id/scrollView1" android:layout_width="match_parent" android:layout_marginTop="10sp">
		<TableLayout android:layout_width="match_parent" android:id="@+id/tableLayout1" android:layout_height="match_parent" android:layout_marginTop="20sp">
			<TableRow android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/TableRow02">
				<LinearLayout android:id="@+id/linearLayout5" android:layout_marginTop="20sp" android:layout_width="match_parent" android:layout_height="wrap_content">
					<Button android:layout_height="wrap_content" android:layout_width="wrap_content" android:textSize="20sp" android:text="@string/TextProductorBtnRegresar" android:id="@+id/IdModHistoricoProdCosechaBtnRegresar" android:onClick="OnModHistoricoProdCosechaBtnRegresar_Click"></Button>
					<Button android:id="@+id/button2" android:layout_height="wrap_content" android:layout_width="wrap_content" android:textSize="20sp" android:text="@string/TEXTproductorBtnSalir"></Button>
				</LinearLayout>
			</TableRow>
			<TableRow android:layout_marginLeft="5sp" android:layout_width="wrap_content" android:layout_marginRight="10sp" android:orientation="vertical" android:layout_height="wrap_content" android:id="@+id/TableRow04">
				<LinearLayout android:id="@+id/LinearLayout10" android:layout_marginRight="10sp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:orientation="vertical">
					<LinearLayout android:id="@+id/LinearLayout08" android:layout_marginTop="10sp" android:layout_marginRight="10sp" android:layout_height="wrap_content" android:layout_width="wrap_content">
						<TextView android:id="@+id/TextView09" android:layout_height="wrap_content" android:layout_width="wrap_content" android:gravity="left|right" android:textColor="#000000" android:text="En los años 2002, 2003 y 2009 con fenómeno del niño, en que mes cosechó?" android:inputType="textMultiLine" android:textSize="15sp"></TextView>
					</LinearLayout>
					<LinearLayout android:id="@+id/LinearLayout09" android:layout_marginTop="5sp" android:layout_height="wrap_content" android:layout_width="wrap_content" android:orientation="horizontal">
						<TextView android:id="@+id/TextView11" android:layout_height="wrap_content" android:layout_width="wrap_content" android:gravity="center_horizontal" android:textColor="#FF0000" android:text="Año" android:width="50sp" android:textSize="15sp"></TextView>
						<TextView android:layout_marginRight="10sp" android:layout_height="wrap_content" android:layout_width="wrap_content" android:gravity="right" android:textColor="#000000" android:width="100sp" android:textSize="15sp" android:id="@+id/IdModHistoricoProdCosechaTxvAnioNino" android:text="2002"></TextView>
					</LinearLayout>
					<LinearLayout android:id="@+id/LinearLayout11" android:layout_marginTop="5sp" android:layout_height="wrap_content" android:layout_width="wrap_content" android:orientation="horizontal">
						<TextView android:id="@+id/TextView13" android:layout_height="wrap_content" android:layout_width="wrap_content" android:gravity="center_horizontal" android:textColor="#FF0000" android:text="Mes" android:width="50sp" android:textSize="15sp"></TextView>
						<TextView android:layout_marginRight="10sp" android:layout_height="wrap_content" android:layout_width="wrap_content" android:gravity="right" android:textColor="#000000" android:width="100sp" android:textSize="15sp" android:id="@+id/IdModHistoricoProdCosechaTxvMesNino" android:text="Enero"></TextView>
					</LinearLayout>
					<LinearLayout android:id="@+id/LinearLayout12" android:layout_marginTop="5sp" android:layout_height="wrap_content" android:layout_width="wrap_content" android:orientation="horizontal">
						<Button android:width="50sp" android:layout_height="wrap_content" android:layout_width="wrap_content" android:textSize="20sp" android:text="&gt;" android:id="@+id/IdModHistoricoProdCosechaBtnNino" android:onClick="OnModHistoricoProdCosechaBtnNino_Click"></Button>
						<RadioGroup android:layout_height="wrap_content" android:layout_width="wrap_content" android:orientation="horizontal" android:id="@+id/IdModHistoricoProdCosechaRdgNino">
							<RadioButton android:layout_width="wrap_content" android:layout_height="wrap_content" android:checked="true" android:textColor="#000000" android:text="Si" android:id="@+id/IdModHistoricoProdCosechaRdoNinoSI"></RadioButton>
							<RadioButton android:layout_width="wrap_content" android:layout_height="wrap_content" android:textColor="#000000" android:text="No" android:id="@+id/IdModHistoricoProdCosechaRdoNinoNO"></RadioButton>
						</RadioGroup>
					</LinearLayout>
				</LinearLayout>
			</TableRow>
			<TableRow android:layout_marginLeft="5sp" android:layout_width="wrap_content" android:layout_marginRight="10sp" android:orientation="vertical" android:layout_height="wrap_content" android:id="@+id/TableRow03">
				<LinearLayout android:id="@+id/LinearLayout04" android:layout_marginRight="10sp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:orientation="vertical">
					<LinearLayout android:id="@+id/LinearLayout02" android:layout_marginTop="10sp" android:layout_marginRight="10sp" android:layout_height="wrap_content" android:layout_width="wrap_content">
						<TextView android:id="@+id/TextView04" android:layout_height="wrap_content" android:layout_width="wrap_content" android:gravity="left|right" android:textColor="#000000" android:inputType="textMultiLine" android:textSize="15sp" android:text="En los años 200, 2001 y 2010 con fenómeno de la niña, en que meses cosechó?"></TextView>
					</LinearLayout>
					<LinearLayout android:id="@+id/LinearLayout03" android:layout_marginTop="5sp" android:layout_height="wrap_content" android:layout_width="wrap_content" android:orientation="horizontal">
						<TextView android:id="@+id/TextView06" android:layout_height="wrap_content" android:layout_width="wrap_content" android:gravity="center_horizontal" android:textColor="#FF0000" android:text="Año" android:width="50sp" android:textSize="15sp"></TextView>
						<TextView android:layout_marginRight="10sp" android:layout_height="wrap_content" android:layout_width="wrap_content" android:gravity="right" android:textColor="#000000" android:width="100sp" android:textSize="15sp" android:id="@+id/IdModHistoricoProdCosechaTxvAnioNina" android:text="2000"></TextView>
					</LinearLayout>
					<LinearLayout android:id="@+id/LinearLayout05" android:layout_marginTop="5sp" android:layout_height="wrap_content" android:layout_width="wrap_content" android:orientation="horizontal">
						<TextView android:id="@+id/TextView08" android:layout_height="wrap_content" android:layout_width="wrap_content" android:gravity="center_horizontal" android:textColor="#FF0000" android:text="Mes" android:width="50sp" android:textSize="15sp"></TextView>
						<TextView android:layout_marginRight="10sp" android:layout_height="wrap_content" android:layout_width="wrap_content" android:gravity="right" android:textColor="#000000" android:width="100sp" android:textSize="15sp" android:id="@+id/IdModHistoricoProdCosechaTxvMesNina" android:text="Enero"></TextView>
					</LinearLayout>
					<LinearLayout android:id="@+id/LinearLayout06" android:layout_marginTop="5sp" android:layout_height="wrap_content" android:layout_width="wrap_content" android:orientation="horizontal">
						<Button android:width="50sp" android:layout_height="wrap_content" android:layout_width="wrap_content" android:textSize="20sp" android:text="&gt;" android:id="@+id/IdModHistoricoProdCosechaBtnNina" android:onClick="OnModHistoricoProdCosechaBtnNina_Click"></Button>
						<RadioGroup android:layout_height="wrap_content" android:layout_width="wrap_content" android:orientation="horizontal" android:id="@+id/IdModHistoricoProdCosechaRdgNina">
							<RadioButton android:layout_width="wrap_content" android:layout_height="wrap_content" android:checked="true" android:textColor="#000000" android:text="Si" android:id="@+id/IdModHistoricoProdCosechaRdoNinaSI"></RadioButton>
							<RadioButton android:layout_width="wrap_content" android:layout_height="wrap_content" android:textColor="#000000" android:text="No" android:id="@+id/IdModHistoricoProdCosechaRdoNinaNO"></RadioButton>
						</RadioGroup>
					</LinearLayout>
				</LinearLayout>
			</TableRow>
			<TableRow android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/TableRow01" android:orientation="vertical" android:layout_marginLeft="5sp" android:layout_marginRight="10sp">
				<LinearLayout android:layout_height="wrap_content" android:id="@+id/linearLayout1" android:orientation="vertical" android:layout_marginRight="10sp" android:layout_width="wrap_content">
					<LinearLayout android:layout_width="wrap_content" android:layout_marginTop="10sp" android:layout_height="wrap_content" android:id="@+id/linearLayout4" android:layout_marginRight="10sp">
						<TextView android:id="@+id/textView1" android:textColor="#000000" android:layout_width="wrap_content" android:layout_height="wrap_content" android:textSize="15sp" android:inputType="textMultiLine" android:gravity="left|right" android:text="Normalmente en que meses se presenta la floración?"></TextView>
					</LinearLayout>
					<LinearLayout android:orientation="horizontal" android:layout_height="wrap_content" android:id="@+id/LinearLayout01" android:layout_marginTop="5sp" android:layout_width="wrap_content">
						<TextView android:id="@+id/TextView03" android:layout_width="wrap_content" android:layout_height="wrap_content" android:textSize="15sp" android:gravity="center_horizontal" android:textColor="#FF0000" android:text="Mes" android:width="50sp"></TextView>
						<TextView android:layout_marginRight="10sp" android:layout_height="wrap_content" android:layout_width="wrap_content" android:textColor="#000000" android:textSize="15sp" android:gravity="right" android:width="100sp" android:id="@+id/IdModHistoricoProdCosechaTxvMesFloracion" android:text="Enero"></TextView>
					</LinearLayout>
					<LinearLayout android:id="@+id/LinearLayout07" android:layout_marginTop="5sp" android:layout_height="wrap_content" android:layout_width="wrap_content" android:orientation="horizontal">
						<Button android:layout_height="wrap_content" android:layout_width="wrap_content" android:textSize="20sp" android:text="&gt;" android:width="50sp" android:id="@+id/IdModHistoricoProdCosechaBtnFloracion" android:onClick="OnModHistoricoProdCosechaBtnFloracion_Click"></Button>
						<RadioGroup android:orientation="horizontal" android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/IdModHistoricoProdCosechaRdgFloracion">
							<RadioButton android:layout_width="wrap_content" android:layout_height="wrap_content" android:checked="true" android:textColor="#000000" android:text="Si" android:id="@+id/IdModHistoricoProdCosechaRdoFloracionSI"></RadioButton>
							<RadioButton android:layout_width="wrap_content" android:layout_height="wrap_content" android:textColor="#000000" android:text="No" android:id="@+id/IdModHistoricoProdCosechaRdoFloracionNO"></RadioButton>
						</RadioGroup>
					</LinearLayout>
				</LinearLayout>
			</TableRow>
			<TableRow android:id="@+id/tableRow3" android:layout_width="wrap_content" android:layout_height="wrap_content">
				<LinearLayout android:id="@+id/linearLayout3" android:layout_height="wrap_content" android:layout_width="match_parent" android:layout_marginTop="30sp">
					<ImageView android:src="@drawable/asohofrucol" android:layout_marginLeft="10sp" android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/imageView2"></ImageView>
					<ImageView android:src="@drawable/um" android:layout_marginLeft="10sp" android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/imageView3"></ImageView>
					<ImageView android:src="@drawable/fondo" android:layout_marginLeft="10sp" android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/imageView4"></ImageView>
				</LinearLayout>
			</TableRow>
		</TableLayout>
	</ScrollView>
</LinearLayout>
