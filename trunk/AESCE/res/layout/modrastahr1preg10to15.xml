<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	android:orientation="vertical" android:background="#FFFFFF">
	<LinearLayout android:id="@+id/linearLayout1"
		android:layout_width="match_parent" android:layout_height="wrap_content">
		<TextView android:id="@+id/textView1" android:layout_height="wrap_content"
			android:background="#7D4312" android:text="Hoja de Respuestas No. 1"
			android:textColor="#FFFFFF" android:layout_width="match_parent"
			android:gravity="center" android:textSize="20sp" android:height="40sp"></TextView>
	</LinearLayout>
	<ScrollView android:id="@+id/scrollView1"
		android:layout_marginTop="10sp" android:layout_width="match_parent"
		android:layout_height="match_parent">
		<LinearLayout android:id="@+id/linearLayout4"
			android:layout_width="match_parent" android:layout_height="match_parent">
			<TableLayout android:layout_width="match_parent"
				android:id="@+id/tableLayout1" android:layout_height="match_parent"
				android:layout_marginLeft="5sp" android:layout_marginRight="10sp">
				<TableRow android:layout_width="wrap_content" android:id="@+id/tableRow3"
					android:layout_height="wrap_content">
					<LinearLayout android:layout_height="wrap_content"
						android:id="@+id/linearLayout5" android:layout_width="match_parent">
						<Button android:textSize="20sp" android:layout_width="wrap_content"
							android:text="@string/TextProductorBtnRegresar"
							android:layout_height="wrap_content" android:id="@+id/Idmodrastahr1preg10to15BtnRegresar"
							android:onClick="Onmodrastahr1preg10to15BtnRegresar_Cllick"></Button>
						<Button android:id="@+id/button2" android:textSize="20sp"
							android:layout_width="wrap_content" android:text="@string/TEXTproductorBtnSalir"
							android:layout_height="wrap_content"></Button>
					</LinearLayout>
				</TableRow>
				<TableRow android:layout_width="wrap_content" android:id="@+id/tableRow1"
					android:layout_height="wrap_content">
					<LinearLayout android:layout_height="wrap_content"
						android:id="@+id/linearLayout3" android:layout_marginTop="20sp"
						android:orientation="vertical" android:layout_width="fill_parent">
						<TextView android:id="@+id/textView2"
							android:layout_height="wrap_content" android:textColor="#704312"
							android:background="#dca76d" android:width="300sp"
							android:gravity="center" android:height="30sp"
							android:layout_width="wrap_content" android:text="Preguntas (10 - 15)"></TextView>
					</LinearLayout>
				</TableRow>
				<TableRow android:layout_width="wrap_content" android:id="@+id/tableRow4"
					android:layout_height="wrap_content" android:layout_marginTop="10sp">
					<LinearLayout android:layout_width="wrap_content"
						android:id="@+id/LinearLayout07" android:layout_height="wrap_content"
						android:orientation="vertical">
						<TextView android:layout_width="wrap_content"
							android:textColor="#000000" android:id="@+id/TextView05"
							android:layout_height="wrap_content" android:layout_marginRight="5sp"
							android:text="10. ¿Observa erosión en el suelo?"
							android:textSize="15sp"></TextView>
						<RadioGroup android:layout_width="wrap_content"
							android:orientation="horizontal" android:layout_height="wrap_content"
							android:id="@+id/Idmodrastahr1preg10to15Rdg10">
							<RadioButton android:checked="true"
								android:textColor="#000000" android:layout_height="wrap_content"
								android:layout_width="wrap_content" android:text="Si"
								android:textSize="15sp" android:id="@+id/Idmodrastahr1preg10to15Rdo10SI"></RadioButton>
							<RadioButton android:layout_marginLeft="10sp"
								android:textColor="#000000" android:layout_height="wrap_content"
								android:layout_width="wrap_content" android:text="No"
								android:textSize="15sp" android:id="@+id/Idmodrastahr1preg10to15Rdo10NO"></RadioButton>
						</RadioGroup>
					</LinearLayout>
				</TableRow>
				<TableRow android:layout_width="wrap_content" android:id="@+id/TableRow01"
					android:layout_height="wrap_content" android:layout_marginTop="10sp">
					<LinearLayout android:layout_height="wrap_content"
						android:id="@+id/LinearLayout01" android:layout_width="wrap_content"
						android:orientation="vertical">
						<TextView android:layout_width="wrap_content"
							android:text="11. ¿Ha observado moho o capas verdosas en la superficie del suelo?"
							android:textColor="#000000" android:id="@+id/TextView01"
							android:layout_height="wrap_content" android:layout_marginRight="5sp"
							android:textSize="15sp"></TextView>
						<RadioGroup android:layout_width="wrap_content"
							android:layout_height="wrap_content" android:orientation="horizontal"
							android:id="@+id/Idmodrastahr1preg10to15Rdg11">
							<RadioButton android:text="Si" android:textColor="#000000"
								android:textSize="15sp" android:layout_width="wrap_content"
								android:checked="true" android:layout_height="wrap_content"
								android:id="@+id/Idmodrastahr1preg10to15Rdo11SI"></RadioButton>
							<RadioButton android:text="No" android:textColor="#000000"
								android:textSize="15sp" android:layout_width="wrap_content"
								android:layout_height="wrap_content" android:layout_marginLeft="10sp"
								android:id="@+id/Idmodrastahr1preg10to15Rdo11NO"></RadioButton>
						</RadioGroup>
					</LinearLayout>
				</TableRow>
				<TableRow android:layout_width="wrap_content" android:id="@+id/TableRow02"
					android:layout_height="wrap_content" android:layout_marginTop="10sp">
					<LinearLayout android:layout_height="wrap_content"
						android:id="@+id/LinearLayout02" android:layout_width="wrap_content"
						android:orientation="vertical">
						<TextView android:layout_width="wrap_content" android:id="@+id/TextView02"
							android:text="12. ¿Observa en la superficie, costras dras o crujientes?"
							android:textColor="#000000" android:textSize="15sp"
							android:layout_marginRight="5sp" android:layout_height="wrap_content"></TextView>
						<Spinner android:layout_width="match_parent" android:layout_height="wrap_content" android:id="@+id/Idmodrastahr1preg10to15spn12"></Spinner>
					</LinearLayout>
				</TableRow>
				<TableRow android:layout_width="wrap_content" android:id="@+id/TableRow09"
					android:layout_marginTop="10sp" android:layout_height="wrap_content">
					<LinearLayout android:layout_height="wrap_content"
						android:layout_width="wrap_content" android:id="@+id/LinearLayout09"
						android:orientation="vertical">
						<TextView android:textSize="15sp" android:textColor="#000000" android:id="@+id/TextView03" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="5sp" android:text="13. El sitio de muestreo se encuentra expuesto al sol durante:"></TextView>
						<Spinner android:layout_width="match_parent" android:layout_height="wrap_content" android:id="@+id/Idmodrastahr1preg10to15spn13"></Spinner>
					</LinearLayout>
				</TableRow>
				<TableRow android:layout_width="wrap_content" android:id="@+id/TableRow04"
					android:layout_marginTop="10sp" android:layout_height="wrap_content">
					<LinearLayout android:layout_height="wrap_content"
						android:layout_width="wrap_content" android:id="@+id/LinearLayout08"
						android:orientation="vertical">
						<TextView android:textColor="#000000" android:id="@+id/TextView06" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="5sp" android:text="14. ¿Observa used costras blancas o peladeros?" android:textSize="15sp"></TextView>
						<Spinner android:layout_width="match_parent" android:layout_height="wrap_content" android:id="@+id/Idmodrastahr1preg10to15spn14"></Spinner>
					</LinearLayout>
				</TableRow>
				<TableRow android:layout_width="wrap_content" android:id="@+id/TableRow03"
					android:layout_marginTop="10sp" android:layout_height="wrap_content">
					<LinearLayout android:layout_height="wrap_content"
						android:layout_width="wrap_content" android:id="@+id/LinearLayout05"
						android:orientation="vertical">
						<TextView android:textColor="#000000" android:id="@+id/TextView04" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="5sp" android:text="15. ¿Observa costras de color negro en el suelo?" android:textSize="15sp"></TextView>
						<Spinner android:layout_width="match_parent" android:layout_height="wrap_content" android:id="@+id/Idmodrastahr1preg10to15spn15"></Spinner>
					</LinearLayout>
				</TableRow>
				<TableRow android:layout_width="wrap_content" android:id="@+id/TableRow08"
					android:layout_marginTop="10sp" android:layout_height="wrap_content">
					<LinearLayout android:layout_height="wrap_content"
						android:layout_width="wrap_content" android:id="@+id/LinearLayout03"
						android:orientation="vertical">
						<Button android:layout_width="wrap_content"
							android:textSize="20sp" android:layout_height="wrap_content"
							android:text="Registrar" android:id="@+id/Idmodrastahr1preg10to15BtnRegistrar"
							android:onClick="Onmodrastahr1preg10to15BtnRegistrar_Click"></Button>
					</LinearLayout>
				</TableRow>
				<TableRow android:layout_width="wrap_content" android:id="@+id/tableRow2"
					android:layout_marginRight="10sp" android:layout_height="wrap_content">
					<LinearLayout android:layout_height="wrap_content"
						android:layout_width="wrap_content" android:id="@+id/LinearLayout04"
						android:layout_marginTop="30sp" android:layout_marginLeft="5sp"
						android:layout_marginBottom="10sp" android:layout_marginRight="10sp">
						<ImageView android:layout_width="wrap_content"
							android:layout_marginLeft="10sp" android:id="@+id/imageView2"
							android:src="@drawable/asohofrucol" android:layout_height="wrap_content"></ImageView>
						<ImageView android:layout_width="wrap_content"
							android:layout_marginLeft="10sp" android:id="@+id/imageView3"
							android:src="@drawable/um" android:layout_height="wrap_content"></ImageView>
						<ImageView android:layout_width="wrap_content"
							android:layout_marginLeft="10sp" android:id="@+id/imageView4"
							android:src="@drawable/fondo" android:layout_height="wrap_content"></ImageView>
					</LinearLayout>
				</TableRow>
			</TableLayout>
		</LinearLayout>
	</ScrollView>
</LinearLayout>
