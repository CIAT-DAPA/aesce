<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	android:scrollbarAlwaysDrawHorizontalTrack="true" android:background="#FFFFFF"
	android:orientation="vertical">
	<ScrollView android:layout_height="wrap_content" android:id="@+id/scrollView1"
		android:layout_width="match_parent" android:layout_marginTop="10sp">
		<TableLayout android:layout_width="match_parent"
			android:id="@+id/tableLayout1" android:layout_height="match_parent"
			android:layout_marginTop="20sp">
			<TableRow android:id="@+id/tableRow2" android:layout_width="wrap_content"
				android:layout_height="wrap_content">
				<LinearLayout android:id="@+id/linearLayout6"
					android:layout_height="wrap_content" android:layout_marginTop="10sp"
					android:layout_width="match_parent" android:layout_marginLeft="5sp">
					<Button android:layout_width="wrap_content"
						android:layout_height="wrap_content" android:text="@string/TEXTproductorNuevoFechaRegis"
						android:textSize="15sp" android:id="@+id/IdModMangoBtnFechaRegis" android:onClick="OnModMangoBtnFechaRegis_Click"></Button>
					<TextView android:layout_width="wrap_content"
						android:layout_height="wrap_content" android:textColor="#000000"
						android:textSize="20sp" android:id="@+id/IdModMangoTxvFecha"></TextView>
				</LinearLayout>
			</TableRow>
			<TableRow android:id="@+id/tableRow5"
				android:layout_marginTop="10sp" android:layout_height="wrap_content"
				android:layout_width="wrap_content">
				<LinearLayout android:id="@+id/linearLayout8"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:layout_marginLeft="5sp" android:layout_marginRight="10sp">
					<TextView android:layout_width="wrap_content"
						android:layout_height="wrap_content" android:id="@+id/TextView01"
						android:textSize="20sp" android:background="#1E3C7B"
						android:textColor="#FFFFFF" android:textStyle="bold"
						android:width="310sp" android:text="@string/TEXTmodCitricosTxvDatosUnidadManejoLote"></TextView>
				</LinearLayout>
			</TableRow>
			<TableRow android:id="@+id/tableRow6" android:layout_height="wrap_content"
				android:layout_width="wrap_content" android:layout_marginTop="10sp">
				<LinearLayout android:id="@+id/linearLayout10"
					android:layout_height="wrap_content" android:layout_width="match_parent"
					android:orientation="vertical" android:layout_marginLeft="5sp"
					android:layout_marginRight="10sp">
					<LinearLayout android:id="@+id/linearLayout7"
						android:background="#FFFFFF" android:layout_height="fill_parent"
						android:layout_width="match_parent" android:layout_marginTop="5sp"
						android:orientation="vertical">
						<LinearLayout android:layout_height="wrap_content"
							android:id="@+id/LinearLayout02" android:layout_width="match_parent">
							<TextView android:layout_height="wrap_content"
								android:layout_marginTop="5sp" android:layout_width="wrap_content"
								android:textSize="15sp" android:id="@+id/textView3"
								android:textColor="#000000" android:gravity="right"
								android:text="@string/TEXTmodCitricosTxvLoteNo" android:width="140sp"></TextView>
							<EditText android:layout_height="wrap_content"
								android:inputType="number" android:layout_width="wrap_content"
								android:layout_marginLeft="5sp" android:layout_marginRight="10sp" android:width="160sp" android:id="@+id/IdModMangoEdtLoteNo"></EditText>
						</LinearLayout>
						<LinearLayout android:layout_height="wrap_content"
							android:id="@+id/LinearLayout06" android:layout_width="match_parent">
							<TextView android:layout_height="wrap_content"
								android:layout_marginTop="5sp" android:layout_width="wrap_content"
								android:textSize="15sp" android:id="@+id/TextView07"
								android:textColor="#000000" android:text="@string/TEXTmodCitricosTxvNArboles"
								android:width="140sp" android:gravity="right"></TextView>
							<EditText android:layout_height="wrap_content"
								android:layout_width="wrap_content" android:layout_marginLeft="5sp"
								android:layout_marginRight="10sp" android:width="160sp"
								android:inputType="number" android:id="@+id/IdModMangoEdtNArboles"></EditText>
						</LinearLayout>
						<LinearLayout android:layout_height="wrap_content"
							android:layout_width="match_parent" android:id="@+id/LinearLayout01">
							<TextView android:id="@+id/TextView02" android:textSize="15sp"
								android:textColor="#000000" android:layout_height="wrap_content"
								android:text="Area del lote en hectareas"
								android:layout_marginTop="5sp" android:layout_width="wrap_content"
								android:gravity="right" android:width="180sp"></TextView>
							<EditText android:layout_marginRight="10sp" android:layout_marginLeft="5sp"
								android:layout_height="wrap_content" android:inputType="numberDecimal" android:layout_width="wrap_content" android:width="120sp" android:id="@+id/IdModMangoEdtAreaLote"></EditText>
						</LinearLayout>
						<LinearLayout android:layout_height="wrap_content"
							android:layout_width="match_parent" android:id="@+id/LinearLayout03">
							<TextView android:id="@+id/TextView03" android:textSize="15sp"
								android:textColor="#000000" android:layout_height="wrap_content"
								android:text="Datos GPS:" android:layout_marginTop="5sp"
								android:layout_width="wrap_content" android:gravity="left"
								android:width="180sp"></TextView>
						</LinearLayout>
						<LinearLayout android:layout_height="wrap_content"
							android:layout_width="match_parent" android:id="@+id/LinearLayout04">
							<TextView android:id="@+id/TextView04" android:textSize="15sp"
								android:textColor="#000000" android:layout_height="wrap_content"
								android:text="Altitud" android:layout_marginTop="5sp"
								android:layout_width="wrap_content" android:gravity="right"
								android:width="140sp"></TextView>
							<EditText android:width="160sp"
								android:layout_marginRight="10sp" android:layout_marginLeft="5sp"
								android:layout_height="wrap_content" android:layout_width="wrap_content"
								android:editable="false" android:id="@+id/IdModMangoEdtAltitud"></EditText>
						</LinearLayout>
						<LinearLayout android:layout_height="wrap_content"
							android:layout_width="match_parent" android:id="@+id/LinearLayout05">
							<TextView android:id="@+id/TextView05" android:textSize="15sp"
								android:textColor="#000000" android:layout_height="wrap_content"
								android:text="Latitud" android:layout_marginTop="5sp"
								android:layout_width="wrap_content" android:gravity="right"
								android:width="140sp"></TextView>
							<EditText android:width="160sp"
								android:layout_marginRight="10sp" android:layout_marginLeft="5sp"
								android:layout_height="wrap_content" android:layout_width="wrap_content"
								android:editable="false" android:id="@+id/IdModMangoEdtLatitud"></EditText>
						</LinearLayout>
						<LinearLayout android:layout_height="wrap_content"
							android:layout_width="match_parent" android:id="@+id/LinearLayout07">
							<TextView android:id="@+id/TextView06" android:textSize="15sp"
								android:textColor="#000000" android:layout_height="wrap_content"
								android:text="Longitud" android:layout_marginTop="5sp"
								android:layout_width="wrap_content" android:gravity="right"
								android:width="140sp"></TextView>
							<EditText android:width="160sp"
								android:layout_marginRight="10sp" android:layout_marginLeft="5sp"
								android:layout_height="wrap_content" android:layout_width="wrap_content"
								android:editable="false" android:id="@+id/IdModMangoEdtLongitud"></EditText>
						</LinearLayout>
					</LinearLayout>
				</LinearLayout>
			</TableRow>
			<TableRow android:id="@+id/TableRow04" android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_marginTop="10sp">
				<LinearLayout android:layout_width="match_parent" android:id="@+id/LinearLayout10" android:layout_height="wrap_content" android:orientation="vertical">
					<TextView android:layout_marginTop="5sp" android:textSize="15sp" android:text="Diligencie la columna que corresponda para los árboles de este lote si ellos están dispersos o claramente definidos a una distancia de siembra" android:textColor="#000000" android:id="@+id/TextView08" android:gravity="left|right" android:layout_width="wrap_content" android:layout_marginRight="10sp" android:layout_height="wrap_content" android:width="300sp" android:layout_marginLeft="5sp"></TextView>
					<RadioGroup android:layout_width="wrap_content" android:layout_height="wrap_content" android:orientation="horizontal" android:id="@+id/IdModMangoRdgDisPersosLtdDefinido">
						<RadioButton android:layout_width="wrap_content" android:textColor="#000000" android:layout_height="wrap_content" android:text="Dispersos" android:id="@+id/IdModMangoRdoDispersos"></RadioButton>
						<RadioButton android:layout_width="wrap_content" android:textColor="#000000" android:layout_height="wrap_content" android:text="Lote definido" android:id="@+id/IdModMangoRdoLtdDefinido"></RadioButton>
					</RadioGroup>
				</LinearLayout>
			</TableRow>
			<TableRow android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_marginTop="10sp">
				<LinearLayout android:layout_marginLeft="5sp" android:layout_width="match_parent" android:id="@+id/LinearLayout08" android:layout_height="wrap_content" android:layout_marginRight="10sp" android:orientation="vertical">
					<LinearLayout android:layout_marginTop="5sp" android:layout_width="match_parent" android:layout_height="fill_parent" android:id="@+id/LinearLayout09" android:background="#FFFFFF" android:orientation="vertical">
						<LinearLayout android:layout_marginBottom="5sp" android:layout_width="match_parent" android:layout_height="wrap_content" android:id="@+id/LinearLayout11">
							<TextView android:textSize="15sp" android:text="Lote definido" android:textColor="#FFFFFF" android:textStyle="bold" android:id="@+id/TextView09" android:background="#1E3C7B" android:layout_width="wrap_content" android:layout_marginRight="10sp" android:layout_height="wrap_content" android:width="310sp"></TextView>
						</LinearLayout>
						<LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content" android:id="@+id/LinearLayout08">
							<TextView android:layout_marginTop="5sp" android:textSize="15sp" android:text="Distancia de siembra (m)" android:textColor="#000000" android:id="@+id/TextView08" android:gravity="right" android:layout_width="wrap_content" android:layout_height="wrap_content" android:width="170sp"></TextView>
							<EditText android:layout_weight="1" android:width="130sp" android:layout_width="wrap_content" android:layout_marginRight="10sp" android:inputType="numberDecimal" android:layout_height="wrap_content" android:layout_marginLeft="5sp" android:id="@+id/IdModMangoEdtDistSiembra"></EditText>
						</LinearLayout>
						<LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content" android:id="@+id/LinearLayout09">
							<RadioGroup android:layout_width="wrap_content" android:layout_height="wrap_content" android:orientation="horizontal" android:id="@+id/IdModMangoRdgLoteDefinido">
								<RadioButton android:layout_width="wrap_content" android:textColor="#000000" android:checked="true" android:layout_height="wrap_content" android:text="Cuadro/rectangulo" android:id="@+id/IdModMangoRdoCuaReg"></RadioButton>
								<RadioButton android:layout_width="wrap_content" android:textColor="#000000" android:layout_height="wrap_content" android:layout_marginLeft="5sp" android:text="Tresbolillo" android:id="@+id/IdModMangoRdoTresbolillo"></RadioButton>
							</RadioGroup>
						</LinearLayout>
						<LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content" android:id="@+id/LinearLayout12" android:orientation="vertical">
							<TextView android:layout_marginTop="5sp" android:textSize="15sp" android:text="Si los citricos esan en asocio con otros cultivos, diga cuales" android:textColor="#000000" android:id="@+id/TextView09" android:gravity="left|right" android:layout_width="wrap_content" android:layout_marginRight="10sp" android:layout_height="wrap_content" android:width="300sp" android:layout_marginLeft="5sp"></TextView>
							<LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content" android:id="@+id/LinearLayout10" android:orientation="horizontal">
								<EditText android:layout_weight="1" android:width="500sp" android:layout_width="wrap_content" android:layout_marginRight="10sp" android:inputType="textMultiLine" android:layout_height="wrap_content" android:height="100sp" android:id="@+id/IdModMangoEdtCulAso"></EditText>
							</LinearLayout>
						</LinearLayout>
					</LinearLayout>
				</LinearLayout>
			</TableRow>
			<TableRow android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_marginTop="10sp">
				<LinearLayout android:layout_marginLeft="5sp" android:layout_width="match_parent" android:id="@+id/LinearLayout25" android:layout_height="wrap_content" android:layout_marginRight="10sp" android:orientation="vertical">
					<LinearLayout android:layout_marginTop="5sp" android:layout_width="match_parent" android:layout_height="fill_parent" android:id="@+id/LinearLayout26" android:background="#FFFFFF" android:orientation="vertical">
						<LinearLayout android:layout_marginBottom="5sp" android:layout_width="match_parent" android:layout_height="wrap_content" android:id="@+id/LinearLayout28">
							<TextView android:textSize="15sp" android:text="Dispersos" android:textColor="#FFFFFF" android:textStyle="bold" android:id="@+id/TextView18" android:background="#1E3C7B" android:layout_width="wrap_content" android:layout_marginRight="10sp" android:layout_height="wrap_content" android:width="310sp"></TextView>
						</LinearLayout>
						<LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content" android:id="@+id/LinearLayout13" android:orientation="vertical">
							<RadioGroup android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/IdModMangoRdgDispersos">
								<RadioButton android:layout_width="wrap_content" android:textColor="#000000" android:checked="true" android:layout_height="wrap_content" android:text="A lo largo de una cerca" android:id="@+id/IdModMangoRdoCerca"></RadioButton>
								<RadioButton android:layout_width="wrap_content" android:textColor="#000000" android:layout_height="wrap_content" android:text="A lo largo de una vía" android:id="@+id/IdModMangoRdoVia"></RadioButton>
								<RadioButton android:layout_width="wrap_content" android:textColor="#000000" android:layout_height="wrap_content" android:text="A lo largo de un río" android:id="@+id/IdModMangoRdoRio"></RadioButton>
								<RadioButton android:layout_width="wrap_content" android:textColor="#000000" android:layout_height="wrap_content" android:text="Alrededor de una vivienda" android:id="@+id/IdModMangoRdoVivienda"></RadioButton>
								<RadioButton android:layout_width="wrap_content" android:textColor="#000000" android:layout_height="wrap_content" android:text="Dispersos en un potrero" android:id="@+id/IdModMangoRdoPotrero"></RadioButton>
							</RadioGroup>
						</LinearLayout>
						<LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content" android:id="@+id/LinearLayout29" android:orientation="vertical">
							<TextView android:layout_marginTop="5sp" android:textSize="15sp" android:text="Otra forma Cual?" android:textColor="#000000" android:id="@+id/TextView10" android:gravity="left|right" android:layout_width="wrap_content" android:layout_marginRight="10sp" android:layout_height="wrap_content" android:width="300sp" android:layout_marginLeft="5sp"></TextView>
							<LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content" android:id="@+id/LinearLayout27" android:orientation="horizontal">
								<EditText android:layout_weight="1" android:width="500sp" android:layout_width="wrap_content" android:layout_marginRight="10sp" android:inputType="textMultiLine" android:layout_height="wrap_content" android:height="100sp" android:id="@+id/IdModMangoEdtOtraForma"></EditText>
							</LinearLayout>
						</LinearLayout>
					</LinearLayout>
				</LinearLayout>
			</TableRow>
			<TableRow android:id="@+id/TableRow01"
				android:layout_marginTop="10sp" android:layout_height="wrap_content"
				android:layout_width="wrap_content">
				<LinearLayout android:layout_width="match_parent" android:id="@+id/LinearLayout14" android:layout_height="wrap_content" android:orientation="vertical">
					<TextView android:layout_marginTop="5sp" android:textSize="15sp" android:text="Tiene analisis quimico del suelo de esta UM o lote" android:textColor="#000000" android:id="@+id/TextView11" android:gravity="left|right" android:layout_width="wrap_content" android:layout_marginRight="10sp" android:layout_height="wrap_content" android:width="300sp" android:layout_marginLeft="5sp"></TextView>
					<LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content" android:id="@+id/LinearLayout30" android:orientation="horizontal">
						<RadioGroup android:layout_width="wrap_content" android:layout_height="wrap_content" android:orientation="horizontal" android:id="@+id/IdModMangoRdgAnaQuimico">
							<RadioButton android:layout_width="wrap_content" android:textColor="#000000" android:checked="true" android:layout_height="wrap_content" android:text="Si" android:id="@+id/IdModMangoRdoAnaQuimicoSI"></RadioButton>
							<RadioButton android:layout_width="wrap_content" android:textColor="#000000" android:layout_height="wrap_content" android:layout_marginLeft="10sp" android:text="No" android:id="@+id/IdModMangoRdoAnaQuimicosNO"></RadioButton>
						</RadioGroup>
					</LinearLayout>
				</LinearLayout>
			</TableRow>
			<TableRow android:id="@+id/TableRow02"
				android:layout_marginTop="10sp" android:layout_height="wrap_content"
				android:layout_width="wrap_content">
				<LinearLayout android:id="@+id/LinearLayout15"
					android:orientation="vertical" android:layout_width="match_parent"
					android:layout_height="wrap_content">
					<LinearLayout android:layout_height="fill_parent"
						android:background="#FFFFFF" android:layout_marginTop="5sp"
						android:layout_width="match_parent" android:layout_marginRight="10sp"
						android:id="@+id/LinearLayout16" android:orientation="vertical"
						android:layout_marginLeft="5sp">
						<LinearLayout android:layout_height="wrap_content"
							android:layout_width="match_parent" android:id="@+id/LinearLayout18"
							android:orientation="vertical">
							<TextView android:id="@+id/TextView15" android:textSize="15sp"
								android:textColor="#000000" android:layout_height="wrap_content"
								android:text="Los arboles de esta UM o lote son injertados"
								android:layout_marginTop="5sp" android:layout_width="wrap_content"
								android:gravity="left|right" android:width="300sp"></TextView>
							<LinearLayout android:layout_height="match_parent"
								android:layout_width="wrap_content" android:id="@+id/LinearLayout17">
								<RadioGroup android:layout_height="wrap_content"
									android:orientation="horizontal" android:layout_width="wrap_content" android:id="@+id/IdModMangoRdgUInjerto">
									<RadioButton android:text="Si" android:layout_width="wrap_content"
										android:layout_height="wrap_content" android:checked="true"
										android:textColor="#000000" android:id="@+id/IdModMangoRdoUInjertoSI"></RadioButton>
									<RadioButton android:text="No" android:layout_marginLeft="10sp"
										android:layout_width="wrap_content" android:layout_height="wrap_content"
										android:textColor="#000000" android:id="@+id/IdModMangoRdoUInjertoNO"></RadioButton>
								</RadioGroup>
							</LinearLayout>
						</LinearLayout>
						<LinearLayout android:layout_height="wrap_content"
							android:layout_width="match_parent" android:id="@+id/LinearLayout20"
							android:orientation="vertical">
							<TextView android:id="@+id/TextView12" android:textSize="15sp"
								android:textColor="#000000" android:layout_height="wrap_content"
								android:text="Tipo o nombre del patron usado en esta UM o lote"
								android:layout_marginTop="5sp" android:layout_width="wrap_content"
								android:gravity="left|right" android:width="300sp"></TextView>
							<EditText android:width="160sp"
								android:layout_marginRight="10sp" android:layout_height="wrap_content"
								android:layout_width="wrap_content" android:id="@+id/IdModMangoEdtNPatron"></EditText>
						</LinearLayout>
						<LinearLayout android:layout_height="wrap_content"
							android:layout_width="match_parent" android:id="@+id/LinearLayout19"
							android:orientation="vertical">
							<TextView android:id="@+id/TextView14" android:textSize="15sp"
								android:textColor="#000000" android:layout_height="wrap_content"
								android:text="Variedad sembrada como copa o semilla sexual"
								android:layout_marginTop="5sp" android:layout_width="wrap_content"
								android:gravity="left|right" android:width="300sp"></TextView>
							<EditText android:width="160sp"
								android:layout_marginRight="10sp" android:layout_height="wrap_content"
								android:layout_width="wrap_content" android:inputType="numberDecimal" android:id="@+id/IdModMangoEdtVariedad"></EditText>
						</LinearLayout>
						<LinearLayout android:layout_height="wrap_content"
							android:layout_width="match_parent" android:id="@+id/LinearLayout22"
							android:orientation="vertical">
							<TextView android:id="@+id/TextView16" android:textSize="15sp"
								android:textColor="#000000" android:layout_height="wrap_content"
								android:text="Edad de la UM o lote en años"
								android:layout_marginTop="5sp" android:layout_width="wrap_content"
								android:gravity="left|right" android:width="300sp"></TextView>
							<EditText android:width="160sp"
								android:layout_marginRight="10sp" android:layout_height="wrap_content"
								android:layout_width="wrap_content" android:inputType="numberDecimal" android:id="@+id/IdModMangoEdtEdad"></EditText>
						</LinearLayout>
						<LinearLayout android:layout_height="wrap_content"
							android:layout_width="match_parent" android:id="@+id/LinearLayout21"
							android:orientation="vertical">
							<TextView android:id="@+id/TextView13" android:textSize="15sp"
								android:textColor="#000000" android:layout_height="wrap_content"
								android:text="% de resiembra" android:layout_marginTop="5sp"
								android:layout_width="wrap_content" android:gravity="left|right"
								android:width="140sp"></TextView>
							<EditText android:width="160sp"
								android:layout_marginRight="10sp" android:layout_height="wrap_content"
								android:layout_width="wrap_content" android:inputType="numberDecimal" android:id="@+id/IdModMangoEdtResiembra"></EditText>
						</LinearLayout>
						<LinearLayout android:layout_height="wrap_content"
							android:layout_width="match_parent" android:id="@+id/LinearLayout23"
							android:orientation="vertical">
							<TextView android:id="@+id/TextView17" android:textSize="15sp"
								android:textColor="#000000" android:layout_height="wrap_content"
								android:text="Produccion del ultimo año en kilogramos"
								android:layout_marginTop="5sp" android:layout_width="wrap_content"
								android:gravity="left|right" android:width="300sp"></TextView>
							<LinearLayout android:layout_height="match_parent"
								android:layout_width="wrap_content" android:id="@+id/LinearLayout24">
								<EditText android:layout_height="wrap_content"
									android:layout_width="wrap_content" android:inputType="numberDecimal" android:width="150sp" android:id="@+id/IdModMangoEdtProAnio"></EditText>
								<Spinner android:layout_height="wrap_content"
									android:layout_width="wrap_content" android:id="@+id/IdModMangoSpnProAnioUnidades"></Spinner>
							</LinearLayout>
						</LinearLayout>
					</LinearLayout>
				</LinearLayout>
			</TableRow>
			<TableRow android:id="@+id/tableRow4" android:layout_height="wrap_content"
				android:layout_width="wrap_content" android:layout_marginTop="10sp">
				<LinearLayout android:id="@+id/linearLayout9"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:layout_marginTop="10sp">
					<Button android:layout_width="wrap_content"
						android:layout_height="wrap_content" android:textSize="20sp"
						android:text="@string/TEXTcontactosBtnRegistrar" android:id="@+id/IdModMangoBtnRegistrar" android:onClick="OnModMangoBtnRegistrar_Click"></Button>
					<Button android:textSize="20sp" android:text="@string/TextProductorBtnRegresar" android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/IdModMangoBtnRegresar" android:onClick="OnModMangoBtnRegresar_Click"></Button>
					<Button android:id="@+id/button2" android:textSize="20sp" android:text="@string/TEXTproductorBtnSalir" android:layout_height="wrap_content" android:layout_width="wrap_content"></Button>
				</LinearLayout>
			</TableRow>
			<TableRow android:id="@+id/tableRow3" android:layout_width="wrap_content"
				android:layout_height="wrap_content">
				<LinearLayout android:id="@+id/linearLayout3"
					android:layout_height="wrap_content" android:layout_marginTop="30sp"
					android:layout_width="match_parent" android:layout_marginBottom="10sp">
					<ImageView android:layout_marginLeft="10sp" android:id="@+id/imageView2"
						android:layout_height="wrap_content" android:src="@drawable/asohofrucol"
						android:layout_width="wrap_content"></ImageView>
